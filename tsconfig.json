{
  "compilerOptions": {
    "target": "ES2017",
     "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/api/admin/dashboard/route.js", "app/api/auth/login/route.js", "app/api/clients/create/route.js", "app/api/clients/delete/route.js", "app/api/clients/list/route.js", "app/api/clients/update/route.js", "app/api/fb/accounts/route.js", "app/api/fb/callback/route.js", "app/api/reports/sync/route.js", "scripts/seedAdmin.js", "lib/auth.js"  ],
  "exclude": [
    "node_modules"
  ]
}
